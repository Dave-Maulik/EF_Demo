@model IEnumerable<EF_Demo.Models.Employee>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EmpName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmpAge)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmpDepartment)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EmpName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmpAge)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmpDepartment)
        </td>
    </tr>
}

</table>
